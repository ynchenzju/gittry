COMLOG_PROCNAME : imas
COMLOG_LEVEL : 4
COMLOG_LOGLENGTH : 10240
COMLOG_SELFDEFINE : NOTSHOWN_ADV

COMLOG_DEVICE_NUM : 2
COMLOG_DEVICE0 : NOTICE
COMLOG_DEVICE1 : WF

NOTICE_OPEN : 1
NOTICE_TYPE : AFILE
NOTICE_PATH : ./log
NOTICE_NAME : imas.log
NOTICE_SYSLEVEL : 0
NOTICE_SELFLEVEL : NOTICE
NOTICE_SIZE : 8192
NOTICE_SPLITE_TYPE : DATECUT
NOTICE_DATA_CUTTIME : 30
NOTICE_QUOTA_DAY : 14
NOTICE_DATA_CRONOCUT : 1
NOTICE_RESERVED1 : %Y%m%d%H%M

WF_OPEN : 1
WF_TYPE : AFILE
WF_PATH : ./log
WF_NAME : imas.log.wf
WF_SYSLEVEL : 0
WF_SELFLEVEL : FATAL,WARNING,TRACE,DEBUG
WF_SIZE : 8192
WF_SPLITE_TYPE : DATECUT
WF_DATA_CUTTIME : 1440
WF_QUOTA_DAY : 7
WF_DATA_CRONOCUT : 1
WF_RESERVED1 : %Y%m%d

